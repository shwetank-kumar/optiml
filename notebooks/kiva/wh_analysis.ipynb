    "edate = '2022-10-12'",
    "print(f\"The analysis is carried our for date range {sdate} to {edate}\")",
    "wh_name=\"PROD_WH\"",
    "time_delta=\"hour\"",
    "sdate_load=\"2022-10-10\"",
    "edate_load=\"2022-10-11\"",
    "threshold=2"
